                        <value>import groovy.json.JsonBuilder

import java.text.SimpleDateFormat
import java.time.LocalDateTime
import java.util.logging.Logger


//author Chandrima Bhattacharya


//begin
//getting the content from the sTotal Lines :ession flowfile
def ff=session.get()
if (ff == null) {
    return
}
//Logger log = Logger.getLogger("DunkinMasterDataCSVToDB");
String strInputData = new File("C:\\Dunkins\\ProxyDLL.csv").text

LocalDateTime date = LocalDateTime.now()
String date_time = date.format("yyyy-MM-dd' 'hh:mm:ss.SS")

//println(strInputData)
//println(date)
String[] strRows = strInputData.split("\n")
String version
long start, end
int rowCount = strRows.size()
try {
  start = System.currentTimeMillis();
    for (int i = 1; i &lt; rowCount; i++) {
        //println(strRows[i])
        String[] a
        a = strRows[i].split(',')
        //println(a[0])
        //   println(a[1])

        String str = a[0]
        String storeid = (str.substring(2, 8))
        //println("storeid:" +storeid)

        String Hostname = a[0]
        // println("HostName:" +Hostname )

        String softwareid = "MyLabor-DLLVer"
        // println("softwareid:"+softwareid)

        version = a[1]
        // println("version:"+version)

        String query = "INSERT INTO public.\"proxyDLL\"(store_id, host_name, software_id, version, createts, modifyts)" +
                "VALUES ( '" + storeid + "','" + Hostname + "','" + softwareid + "', '" + version + "','" + date_time + "','" + date_time + "')"
        println(query)
        try {
            SQL.mydb.withTransaction {
                SQL.mydb.execute(query);
                results = "{True}"
				   end = System.currentTimeMillis();
           // results = "{True}"
        log.info("Time taken:"+(end-start)+" ms\n")
            }
        } catch (Exception e) {
          //   log.info("\n Error Loading data for " + query);
           // log.info("........................................");
        }
    }


// End of For Loop i
  //  results = "{True}"
//write results as a new flow file content
ff.write("UTF-8") { writer -&gt;
    new JsonBuilder(results).writeTo(writer)
}
//transfer to success
REL_SUCCESS &lt;&lt; ff
}
catch(ex) {
    results="{False}"
    //log.info('!! Error while Data Audit:', ex)
//write results as a new flow file content
    ff.write("UTF-8"){writer-&gt;
        new JsonBuilder(results).writeTo(writer)
    }
//transfer to failure
    REL_FAILURE &lt;&lt; ff
}</value>
