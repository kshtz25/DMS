<value>import groovy.json.JsonSlurper
import groovy.json.JsonBuilder
import java.time.LocalDateTime
import java.util.logging.Logger;

 

/**
 * @author Pradipta Hore
 *
 */

 

//begin
//getting the content from the sTotal Lines :session flowfile
def ff=session.get()
if (ff == null) {
    return
}

 

Logger log = Logger.getLogger("HostsWithGroupsLog");
String strInputData = new File("C:\\Data_Items\\HostswithGroups.json").text
json_sup = new JsonSlurper()
json_response = json_sup.parseText(strInputData)

 

LocalDateTime date = LocalDateTime.now()
String date_time = date.format("yyyy-MM-dd hh:mm:ss.SSS")
log.info("date="+date_time)

 

int lines1=0, lines=0
String str, store_id, terminal_type, group_nm, rowInDB=""
long startTime, endTime
def flowfiledata
def results = []
ArrayList&lt;String&gt; queries = new ArrayList&lt;String&gt;();

 

try {
    startTime = System.currentTimeMillis();
    for (int i = 0; i &lt; json_response.hosts.size(); i++) {
        //Logic for store_id &amp; terminal_type
        str = json_response.hosts.description[i]

 

        if (str.contains("WSKDS")) {
            store_id = str[-6..-1]
            terminal_type = "KDS"
        }
        else if (str.contains("POS") &amp;&amp; str.length()&gt;11){
            store_id = str.substring(2,8)
            terminal_type = "POS"
        }
        else {
            store_id = str
            terminal_type = "UAT"
        }
        for (int j = 0; j &lt; json_response.groups.size(); j++){
            if (json_response.groups.id[j] == json_response.hosts.groupid[i])
                group_nm = json_response.groups.name[j]
        }

 

        //Preparing SQL Query
        def query = "INSERT INTO public.\"HostswithGroups\"(store_id, group_id, group_name, terminal_id, terminal_desc, \"hostStateChangeDate\", \"isHostOnline\", terminal_type, createts, modifyts) " +
                "VALUES ('"+store_id+
                "','"+json_response.hosts.groupid[i]+
                "','"+group_nm+"','"+json_response.hosts.id[i]+
                "','"+json_response.hosts.description[i]+
                "','"+json_response.hosts.hostStateChangeDate[i]+
                "','"+json_response.hosts.isHostOnline[i]+
                "','"+terminal_type+
                "','"+date_time+
                "','"+date_time+"');"

 

        queries.add(query)
    }// End of For Loop i

 

        //log.info("\n query " + query);
        try {
            SQL.mydb.withTransaction {
                SQL.mydb.withBatch(100) { stmt -&gt;
                    queries.each { query -&gt;
                        stmt.addBatch(query);
                    }
                }
                results = "{True}"
            }
        } catch (Exception e) {
            log.info("\n Error Loading data for " +e);
            log.info("........................................");
            def filename = "HWG_DBerror.txt"
            def ipfile = new File("C:\\Error_Files\\"+filename)
            if(ipfile.exists()) {
                ipfile.append(e.toString())
            }
            else {
                ipfile.write(e.toString())
            }

 

            BufferedReader reader = new BufferedReader(new FileReader("C:\\Error_Files\\HWG_DBerror.txt"));
            while (reader.readLine() != null) lines++;
            reader.close();

 

            results="{False}"
            ff.write("UTF-8"){writer-&gt;
                new JsonBuilder(results).writeTo(writer)
            }
            //transfer to failure
            REL_FAILURE &lt;&lt; ff
        }

 

    endTime = System.currentTimeMillis();

 

    def countQuery = "SELECT COUNT(*) AS \"Success\" FROM public.\"HostswithGroups\""
    SQL.mydb.withTransaction {
        rowInDB = SQL.mydb.firstRow(countQuery);
        //results = "{True}"
    }

 

/*//write results as a new flow file content
    ff.write("UTF-8") { writer -&gt;
        new JsonBuilder(results).writeTo(writer)
    }
//transfer to success
    REL_SUCCESS &lt;&lt; ff*/
}
catch(ex) {
    results="{False}"
    log.info('!! Error while Data Audit:'+ ex.printStackTrace())
    //write results as a new flow file content
    //def content = ex.printStackTrace()
    def filename = "HWG_DataError.txt"
    def ipfile = new File("C:\\Error_Files\\"+filename)
    if(ipfile.exists()) {
        ipfile.write(ex.toString())
    }
    else{
        ipfile.write(ex.toString())
    }
    BufferedReader reader = new BufferedReader(new FileReader("C:\\Error_Files\\"+filename));
    while (reader.readLine() != null) lines1++;
    reader.close();
    ff.write("UTF-8"){writer-&gt;
        new JsonBuilder(results).writeTo(writer)
    }
    //transfer to failure
    REL_FAILURE &lt;&lt; ff
}

 

flowfiledata = " Total No of records in file  HostswithGroups.json : "+json_response.hosts.size()+"\n"+
        " Total records loaded into DB : "+rowInDB.replaceAll("[{|}|:]", "")+"\n"+
        " Total Time taken : "+(endTime-startTime)/1000+" Sec\n"+
        " Total records errored -  \n"+
        "     Database Error : "+lines/2+"\n"+
        "     Data Error : "+lines1+"\n"
if (results=="{True}") {
    ff.write("UTF-8", flowfiledata)
    REL_SUCCESS &lt;&lt; ff
}</value>
